def insertion_sort(array)
    n= array.length
    for i in 1...(n)
        j=i
        while j>0
            if array[j-1]>array[j]
               
                array[j],array[j-1] = array[j-1],array[j]
            else
              break
            end
            j=j-1
        end
    end
    puts array
end
array=[11,43,56,76,34,22]
insertion_sort(array)
